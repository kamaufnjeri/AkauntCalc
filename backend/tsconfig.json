{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",                                /* Specify what module code is generated. */
    "outDir": "./dist",
    "rootDir": "./src",                                 /* Specify the root folder within your source files. */
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
  },
}
